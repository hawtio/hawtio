{
  "name": "hawtio-console-assembly",
  "keywords": [
    "hawtio"
  ],
  "main": [
    "dist/hawtio-console-assembly.js",
    "dist/hawtio-console-assembly.css"
  ],
  "ignore": [
    ".*",
    "dist/hawtio-console-assembly-test.js",
    "**/.*",
    "libs",
    "node_modules",
    "plugins",
    "test-plugins",
    "tslint-rules"
  ],
  "dependencies": {
    "hawtio-integration": "~3.0.0",
    "hawtio-preferences": "~3.0.0",
    "angular-resizable": "~1.2.0",
    "patternfly": "~4.0.0-rc.1"
  },
  "devDependencies": {
    "hawtio-core-dts": "~3.0.0",
    "hawtio-extension-service": "~3.0.0"
  },
  "overrides": {
    "bootstrap": {
